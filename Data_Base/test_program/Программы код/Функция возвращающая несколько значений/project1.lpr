program one;
uses crt;
type heryui=record
     name:string;
     tyu:integer;
end;
var i:integer;
function uru(i:integer):heryui;
begin
  inc(i);
  str(i,uru.name);
  uru.name:=uru.name+'kil';
  writeln(i);
  uru.tyu:=i;
end;
begin
  i:=1;
  writeln(uru(i).tyu);
  write(uru(i).name);
  readln;
end.
